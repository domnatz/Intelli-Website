{
    "version": 2,
    "builds": [
      { 
        "src": "client/build/", 
        "use": "@vercel/static"
      },
      {
        "src": "server/index.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)", 
        "dest": "server/index.js" 
      }
    ]
  }